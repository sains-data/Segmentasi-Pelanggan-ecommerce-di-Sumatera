      "params": {
        "datasource": "gold_segmentasi_pelanggan__table",
        "metrics": ["sum__total_pembayaran"],
        "groupby": ["provinsi_pelanggan", "segment"],
        "title": "Total Pembelian per Provinsi dan Segmen",
        "bar_stacked": true,
        "show_legend": true,
        "x_axis_label": "Provinsi",
        "y_axis_format": ".2s"
      }
    }
  ]
